{
  "#": "Automated accessibility testing ~ https://github.com/pa11y/pa11y-ci",

  "defaults": {
    "debug": true,
    "screen-capture": ".",
    "standard": "WCAG2AA",
    "X-root-element": ".post",
    "threshold": 2,
    "timeout": 5000,
    "X-wait": 2000
